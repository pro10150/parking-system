ue, setValue] = useState([]);\n\n  // Modify the current state by setting the new data to\n  // the response from the backend\n  useEffect(() => {\n    fetch('https://parking-system-iot.herokuapp.com/parking', {\n      'methods': 'GET',\n      headers: {\n        'Content-Type': 'application/json'\n      }\n    })\n      .then(response => response.json())\n      .then(response => setParkings(response))\n      .catch(error => console.log(error))\n\n  }, [])\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    navigate('/' + value);\n    // do what you want with your form data\n    // navigate('/2');\n  }\n\n  const handleParking = (e) => {\n    e.preventDefault();\n    // navigate('/enter');\n    fetch(\"https://parking-system-iot.herokuapp.com/enter\")\n    .then(response => {\n        response.json()\n        .then(data => {\n          let id = data.id\n          navigate('/enter/' + id);\n        })\n    })\n    .catch(response => {\n        console.log(response)\n    })\n  }\n\n  return (\n\n    <Routes>\n      <Route path=\"/\" element={\n        <div className=\"container-xxl\">\n          <div className=\"row\">\n            <div className=\"text-center row gy-5\">\n              <h1 class=\"display-1\">\n                Carparking Online service\n              </h1>\n            </div>\n            \n            <div class='row  gy-5 align-items-center'>\n              <div className='row col-xxl  gy-5 ' >\n                <div class=\"text-center\">\n                  \n                  <img src={process.env.PUBLIC_URL + \"/piccomp/park1.png\"} class=\"rounded mx-auto d-block img-fluid\" />\n                </div>\n              </div>\n\n              <div className='col-xxl' >\n                <div class=\"container-fluid \">\n                  <div class=\"text-center\">\n                  <img src={process.env.PUBLIC_URL + \"/piccomp/car 6.png\"} class=\"rounded mx-auto d-block img-fluid\" width=\"300\"/>\n                    <br /><br />\n 